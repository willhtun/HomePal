<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="willhtun.homepal">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Rent" />
        <activity android:name=".Internet"></activity>
        <activity android:name=".Electricity"></activity>
        <activity android:name=".Water"></activity>
        <activity android:name=".Gas"></activity>
        <activity android:name=".Trash"></activity>
        <activity android:name=".Settings"></activity>

    </application>

</manifest>